{
	"name": "dev-boon",
	"displayName": "dev-boon",
	"description": "An extension which helps developers in order to find solutions to errors occured during execution of commands(in command line) through stackOverFlow,youtube videos etc.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"publisher": "ChiragGupta",
	"activationEvents": [
		"onCommand:dev-boon.STACKOVERFLOW_SEARCH_WITH_SELECTED_TEXT",
		"onCommand:dev-boon.DEACTIVATE_EXTENSION",
		"onCommand:dev-boon.STACKOVERFLOW_SEARCH_WITH_SELECTED_TEXT_USING_PROMPT",
		"onCommand:dev-boon.YOUTUBE_SEARCH_WITH_SELECTED_TEXT",
		"onCommand:dev-boon.YOUTUBE_SEARCH_WITH_SELECTED_TEXT_USING_PROMPT"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "dev-boon.STACKOVERFLOW_SEARCH_WITH_SELECTED_TEXT",
				"category": "dev-boon",
				"title": "stackOverFlow Search without prompt(but only takes selected text)"
			},
			{
				"command": "dev-boon.DEACTIVATE_EXTENSION",
				"category": "dev-boon",
				"title": "deactivate dev-boon extension"
			},
			{
				"command": "dev-boon.STACKOVERFLOW_SEARCH_WITH_SELECTED_TEXT_USING_PROMPT",
				"category": "dev-boon",
				"title": "stackOverFlow Search using prompt"
			},
			{
				"command": "dev-boon.YOUTUBE_SEARCH_WITH_SELECTED_TEXT",
				"category": "dev-boon",
				"title": "youTube Search without prompt(but only takes selected text)"
			},
			{
				"command": "dev-boon.YOUTUBE_SEARCH_WITH_SELECTED_TEXT_USING_PROMPT",
				"category": "dev-boon",
				"title": "youTube Search using prompt"
			}
		],"keybindings":[
			{
				"command": "dev-boon.STACKOVERFLOW_SEARCH_WITH_SELECTED_TEXT",
				"key": "ctrl+h"
			},
			{
				"command": "dev-boon.STACKOVERFLOW_SEARCH_WITH_SELECTED_TEXT_USING_PROMPT",
				"key": "ctrl+a"
			},
			{
				"command": "dev-boon.YOUTUBE_SEARCH_WITH_SELECTED_TEXT",
				"key": "ctrl+g"
			},
			{
				"command": "dev-boon.YOUTUBE_SEARCH_WITH_SELECTED_TEXT_USING_PROMPT",
				"key": "ctrl+f"
			},
			{
				"command": "dev-boon.DEACTIVATE_EXTENSION",
				"key": "ctrl+d"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/request-promise-native": "^1.0.17",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"open": "^8.0.1",
		"request": "^2.88.2",
		"request-promise-native": "^1.0.9"
	}
}
